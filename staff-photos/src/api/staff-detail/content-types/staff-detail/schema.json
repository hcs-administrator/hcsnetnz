{
  "kind": "collectionType",
  "collectionName": "staff_details",
  "info": {
    "singularName": "staff-detail",
    "pluralName": "staff-details",
    "displayName": "StaffDetail",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "staff_id": {
      "type": "uid",
      "default": "101",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "roles": {
      "type": "string",
      "required": true
    },
    "departments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::department.department",
      "mappedBy": "staff_details"
    }
  }
}
